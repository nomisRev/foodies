{
  "realm": "foodies-keycloak",
  "enabled": true,
  "registrationAllowed": true,
  "resetPasswordAllowed": true,
  "eventsEnabled": true,
  "eventsListeners": ["jboss-logging", "profile-webhook"],
  "enabledEventTypes": ["REGISTER"],
  "clients": [
    {
      "clientId": "foodies",
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "secret": "foodies_client_secret",
      "redirectUris": ["http://localhost:8080/oauth/callback"],
      "webOrigins": ["http://localhost:8080"],
      "defaultClientScopes": ["profile", "email", "roles"],
      "audience": ["foodies-webapp"],
      "attributes": {
        "post.logout.redirect.uris": "http://localhost:8080/*"
      }
    },
    {
      "clientId": "foodies-basket",
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "secret": "foodies_basket_secret",
      "audience": ["foodies-basket"]
    },
    {
      "clientId": "foodies-order",
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "secret": "foodies_order_secret",
      "audience": ["foodies-order"]
    }
  ],
  "users": [
    {
      "username": "food_lover",
      "enabled": true,
      "email": "food_lover@gmail.com",
      "firstName": "Food",
      "lastName": "Lover",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": ["user"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Regular user with standard permissions"
      }
    ]
  }
}
