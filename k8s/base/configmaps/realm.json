{
  "realm": "foodies-keycloak",
  "enabled": true,
  "registrationAllowed": true,
  "resetPasswordAllowed": true,
  "eventsEnabled": true,
  "eventsListeners": [
    "jboss-logging",
    "profile-webhook"
  ],
  "enabledEventTypes": [
    "REGISTER",
    "DELETE_ACCOUNT",
    "UPDATE_PROFILE"
  ],
  "clients": [
    {
      "clientId": "foodies",
      "enabled": true,
      "publicClient": false,
      "secret": "foodies_client_secret",
      "redirectUris": [
        "http://${FOODIES_HOST}/oauth/callback",
        "https://${FOODIES_HOST}/oauth/callback"
      ],
      "webOrigins": [
        "http://${FOODIES_HOST}",
        "https://${FOODIES_HOST}"
      ],
      "attributes": {
        "post.logout.redirect.uris": "http://${FOODIES_HOST}/*,https://${FOODIES_HOST}/*"
      }
    }
  ],
  "users": [
    {
      "username": "food_lover",
      "enabled": true,
      "email": "food_lover@gmail.com",
      "firstName": "Food",
      "lastName": "Lover",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Regular user with standard permissions"
      }
    ]
  }
}
