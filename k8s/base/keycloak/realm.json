{
  "realm": "foodies-keycloak",
  "sslRequired": "none",
  "enabled": true,
  "registrationAllowed": true,
  "resetPasswordAllowed": true,
  "eventsEnabled": true,
  "eventsListeners": ["jboss-logging", "profile-webhook"],
  "enabledEventTypes": [
    "REGISTER",
    "DELETE_ACCOUNT",
    "UPDATE_PROFILE"
  ],
  "clients": [
    {
      "clientId": "foodies",
      "protocol": "openid-connect",
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "secret": "foodies_client_secret",
      "defaultClientScopes": [
        "web-origins",
        "roles",
        "profile",
        "email",
        "foodies-audience",
        "aud-basket-service"
      ],
      "optionalClientScopes": [
        "aud-basket-service",
        "aud-menu-service",
        "aud-order-service",
        "aud-payment-service",
        "aud-profile-service"
      ],
      "redirectUris": [
        "http://foodies.local/oauth/callback",
        "https://foodies.com/oauth/callback"
      ],
      "webOrigins": [
        "http://foodies.local",
        "https://foodies.com"
      ],
      "attributes": {
        "post.logout.redirect.uris": "http://foodies.local/##https://foodies.com/"
      }
    },
    {
      "clientId": "basket-service",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "basket_service_client_secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "optionalClientScopes": [
        "aud-basket-service",
        "aud-menu-service",
        "aud-order-service",
        "aud-payment-service",
        "aud-profile-service"
      ]
    },
    {
      "clientId": "menu-service",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "menu_service_client_secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "optionalClientScopes": [
        "aud-basket-service",
        "aud-menu-service",
        "aud-order-service",
        "aud-payment-service",
        "aud-profile-service"
      ]
    },
    {
      "clientId": "order-service",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "order_service_client_secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "optionalClientScopes": [
        "aud-basket-service",
        "aud-menu-service",
        "aud-order-service",
        "aud-payment-service",
        "aud-profile-service"
      ]
    },
    {
      "clientId": "payment-service",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "payment_service_client_secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "optionalClientScopes": [
        "aud-basket-service",
        "aud-menu-service",
        "aud-order-service",
        "aud-payment-service",
        "aud-profile-service"
      ]
    },
    {
      "clientId": "profile-service",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "profile_service_client_secret",
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "optionalClientScopes": [
        "aud-basket-service",
        "aud-menu-service",
        "aud-order-service",
        "aud-payment-service",
        "aud-profile-service"
